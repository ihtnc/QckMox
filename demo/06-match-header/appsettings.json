{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "QckMox": {
    "ResponseMap": {
      "POST summary/1 qmx-user-id=1": "summary\\1.json",
      "POST summary/1 qmx-user-id=3": "summary\\3.json",
      "POST summary/1 qmx-user-id=5": "summary\\5.json",
      "POST summary/1 qmx-user-id=7": "summary\\7.json",
      "POST summary/1 qmx-user-id=9": "summary\\9.json"
    },
    "Request": {
      "MatchHeader": ["user-id"]
    }
  }
}
