{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "QckMox": {
    "EndPoint": "/api/",
    "ResponseMap": {
      "GET weatherforecast": "weatherforecast\\Forecast.json",
      "GET weathersummary id=1": "weathersummary\\1.json",
      "GET weathersummary id=3": "weathersummary\\3.json",
      "GET weathersummary id=5": "weathersummary\\5.json",
      "GET weathersummary id=7": "weathersummary\\7.json",
      "GET weathersummary id=9": "weathersummary\\9.json"
    },
    "Request": {
      "MatchQuery": ["id"]
    }
  }
}
